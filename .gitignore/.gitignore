#include <stdio.h>
int main(){
  int a[5] = {1,2,3,4,5};
  int key;
  int i;
  /*tell the user to input the key they are searching for*/
  printf("Enter the number you want to search for:\n");
  scanf("%d",&key);
  
  for(i=0; i<5; i++){
    if(a[i]==key)
    {
      printf("%d has been found!\n",key);
      break;
    }
    else
    {
      printf("Not Found!\n");
      break;
    }
  }
}
